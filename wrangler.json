{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "whatsapp-reminder",
  "main": "src/index.ts",
  "compatibility_date": "2024-12-30",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true
  },
  "triggers": {
    "crons": [
      "30 2 * * *"
    ]
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "calendar-sql",
      "database_id": "414928a0-fa3e-4f61-9f4d-a2bbe70be63c"
    }
  ]
}
